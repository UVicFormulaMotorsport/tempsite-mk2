              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='#') UVic FSAE
          #navbar.navbar-collapse.collapse
            ul.nav.navbar-nav
              li.active
                a(href='#') Home
              li
                a(href='#about') About
              li
                a(href='#contact') Contact
              li.dropdown
